;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	pedro.br. root.pedro.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	pedro.br.
@	IN	NS	ns1.pedro.br.
@	IN	A	10.24.11.25

@	IN	A	127.0.0.1
;@	IN	AAAA	::1

ns1	IN	A	10.24.11.25